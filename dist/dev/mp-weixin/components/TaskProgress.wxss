  width: 90rpx;
  height: 90rpx;
  color: #857ed1;
  margin-right: 20rpx;
  background-color: #f3f4f6;
  border-radius: 50%;
  font-size: 70rpx;
  box-shadow: 0 5px 15px rgba(46, 54, 80, 0.3);
  overflow: hidden;
  /* 确保水波纹不超出圆形按钮 */
}
.bottom-icon.data-v-defa837f::before {
  content: "";
  position: absolute;
  width: 100%;
  height: 100%;
  border-radius: 50%;
  background: rgba(0, 0, 0, 0.2);
  transform: scale(0);
  opacity: 0;
  pointer-events: none;
  transition: transform 0.6s, opacity 0.6s;
}
.bottom-icon.data-v-defa837f:active::before {
  transform: scale(4);
  opacity: 1;
}
